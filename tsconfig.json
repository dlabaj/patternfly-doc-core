{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "importHelpers": true,
    "verbatimModuleSyntax": false,
    "allowImportingTsExtensions": true
  },
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist", "node_modules"]
}
